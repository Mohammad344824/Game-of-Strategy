import java.io.IOException;
import java.io.PrintWriter;
import java.net.Socket;
import java.util.Scanner;

public class Client2 {
    public static void main(String[] args) {
        try {
            Socket socket = new Socket("localhost", 5555);
            Scanner scanner = new Scanner(socket.getInputStream());
            PrintWriter printWriter = new PrintWriter(socket.getOutputStream(), true);

            // Round 1
            printWriter.println("1 King (13), Diamonds");

            // Round 2
            printWriter.println("2 9, Diamonds");

            // Round 3
            printWriter.println("3 Jack (11), Diamonds");

            // Round 4
            printWriter.println("4 2, Diamonds");

            // Round 5
            printWriter.println("5 4, Diamonds");

            // Round 6
            printWriter.println("6 1, Diamonds");

            // Round 7
            printWriter.println("7 10, Diamonds");

            // Round 8
            printWriter.println("8 3, Diamonds");

            socket.close();
        } catch (IOException e) {
            e.printStackTrace();
        }
    }
}
