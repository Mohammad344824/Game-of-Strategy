import java.io.IOException;
import java.io.PrintWriter;
import java.net.Socket;
import java.util.Scanner;

public class Client1 {
    public static void main(String[] args) {
        try {
            Socket socket = new Socket("localhost", 5555);
            Scanner scanner = new Scanner(socket.getInputStream());
            PrintWriter printWriter = new PrintWriter(socket.getOutputStream(), true);

            // Round 1
            printWriter.println("1 Ace (1), Hearts");

            // Round 2
            printWriter.println("2 2, Hearts");

            // Round 3
            printWriter.println("3 5, Hearts");

            // Round 4
            printWriter.println("4 6, Hearts");

            // Round 5
            printWriter.println("5 Queen (12), Hearts");

            // Round 6
            printWriter.println("6 3, Hearts");

            // Round 7
            printWriter.println("7 4, Hearts");

            // Round 8
            printWriter.println("8 13, Hearts");

            socket.close();
        } catch (IOException e) {
            e.printStackTrace();
        }
    }
}
